{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "source": {
    "include": ["./App.js", "components/Chat.js", "components/Start.js", "components/CustomActions.js"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
    },
    "opts": {
    "template": "templates/default",
    "encoding": "utf8",
    "destination": "./out/",
    "recurse": true,
    "readme": "README.md"
    }
}  